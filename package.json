{
  "name": "synth-react-redux",
  "version": "1.0.0",
  "description": "Synthesizer using React and Redux",
  "main": "src/index.js",
  "scripts": {
    "build": "react-scripts build",
    "lint": "esw --color",
    "lint:watch": "npm run lint -- --watch",
    "open:build": "cd build && serve",
    "postbuild": "mv ./build ./docs",
    "prebuild": "rm -rf ./docs",
    "start": "react-scripts start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruebel/synth-react-redux.git"
  },
  "author": "@ruebel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ruebel/synth-react-redux.git/issues"
  },
  "babel": {
    "presets": ["env", "react", "stage-2"],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "gsandf-react",
    "parser": "babel-eslint",
    "rules": {
      "sort-keys": 0
    }
  },
  "dependencies": {
    "color": "^3.0.0",
    "normalize.css": "8.0.0",
    "rc-slider": "^8.4.1",
    "react": "^16.1.1",
    "react-dimensions": "^1.3.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-scripts": "^1.1.4",
    "react-select": "^1.0.1",
    "react-tooltip": "^3.4.0",
    "redux": "^4.0.0",
    "redux-persist": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.2.6",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-plugin-styled-components": "^1.3.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "connect-history-api-fallback": "1.5.0",
    "eslint": "^4.12.0",
    "eslint-config-gsandf-react": "^1.0.2",
    "eslint-watch": "^3.1.3",
    "npm-run-all": "4.1.2",
    "prettier": "^1.8.2",
    "prop-types": "^15.6.0",
    "redux-immutable-state-invariant": "2.1.0",
    "regenerator": "0.12.3"
  }
}
