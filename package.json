{
  "name": "synth-react-redux",
  "version": "1.0.0",
  "description": "Synthesizer using React and Redux",
  "main": "index.js",
  "scripts": {
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node ./tools/srcServer",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw -f simple-detail --color src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run lint",
    "build": "babel-node tools/build.js",
    "start-message": "babel-node ./tools/startMessage",
    "deploy": "cd dist && git add --all && git commit -m \"Release: $(date)\" && git push",
    "deploy:clean": "rm -rf dist && git clone https://github.com/ruebel/synth-react-redux --branch gh-pages dist && npm run build && npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruebel/synth-react-redux.git"
  },
  "author": "@ruebel",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ruebel/synth-react-redux.git/issues"
  },
  "homepage": "https://github.com/ruebel/synth-react-redux.git#readme",
  "dependencies": {
    "classnames": "2.2.5",
    "normalize.css": "4.2.0",
    "react": "15.3.1",
    "react-addons-css-transition-group": "15.3.1",
    "react-addons-test-utils": "15.3.1",
    "react-dimensions": "1.3.0",
    "react-dom": "15.3.1",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-select": "1.0.0-rc.2",
    "react-slider": "0.7.0",
    "react-tooltip": "^3.2.2",
    "redux": "3.6.0",
    "redux-persist": "^4.0.1",
    "redux-thunk": "2.1.0",
    "uuid": "2.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.14.0",
    "browser-sync": "2.18.2",
    "connect-history-api-fallback": "1.3.0",
    "css-loader": "0.25.0",
    "eslint": "3.5.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "1.15.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.2.2",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "jsdom": "9.5.0",
    "nodemon": "1.10.2",
    "npm-run-all": "3.1.0",
    "postcss-cssnext": "2.8.0",
    "postcss-import": "8.1.2",
    "postcss-loader": "0.13.0",
    "postcss-modules": "0.5.2",
    "postcss-reporter": "1.4.1",
    "postcss-simple-vars": "3.0.0",
    "react-addons-test-utils": "15.3.1",
    "redux-immutable-state-invariant": "1.2.3",
    "regenerator": "0.8.46",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "stylelint": "7.2.0",
    "stylelint-config-standard": "13.0.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.7.0",
    "webpack-dev-server": "1.15.2",
    "webpack-hot-middleware": "2.12.2",
    "webpack-md5-hash": "0.0.5"
  }
}
